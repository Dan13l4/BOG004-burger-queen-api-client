les.cartContainer}>\r\n            <div >\r\n                <div className={styles.buttonCart}>\r\n                    <img src={takeAwayOrder} alt=\"order icon\" style={{ width: 80 }} />\r\n                    <p className={styles.p}># Products: {productsLenght} items</p>\r\n                </div>\r\n            </div>\r\n            <div>\r\n                <h3>Order Cart</h3>\r\n                <table className=\"table table-striped table-dark table-hover\">\r\n                <thead>\r\n                    <tr>\r\n                        <th scope=\"col\">Product:</th>\r\n                        <th scope=\"col\">Quantity:</th>\r\n                        <th scope=\"col\">Price:</th>\r\n                        <th scope=\"col\">Options:</th>\r\n                    </tr>\r\n                </thead>\r\n                {cartItems.length ===0 ? <thead><tr><th scope=\"row\">Empty Cart</th></tr></thead> : (\r\n                    <tbody>\r\n                        {cartItems.map((item, i) => (\r\n                        <ItemCart scope=\"col\" key={i} item={item}/>\r\n                    ))}\r\n                    </tbody>\r\n                    )}\r\n                    <tfoot>\r\n                        <tr>\r\n                            <th scope=\"col\">Total: ${total}</th>\r\n                        </tr>\r\n                    </tfoot>\r\n                    </table>\r\n                    <button className='btn btn-info btn-lg' onClick={()=>transitOrder({cartItems})}>Send Order</button>\r\n            </div>\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default Cart;","C:\\Users\\57322\\Documents\\GitHub\\Proyectos Laboratoria\\BOG004-burger-queen-api-client\\src\\Context\\CartContext.jsx",[],[],"C:\\Users\\57322\\Documents\\GitHub\\Proyectos Laboratoria\\BOG004-burger-queen-api-client\\src\\components\\ItemCart.jsx",[],[],{"ruleId":"79","severity":1,"message":"80","line":6,"column":8,"nodeType":"81","messageId":"82","endLine":6,"endColumn":16},"no-unused-va