;\n\tconst [selectedProduct, setSelectedProduct] = useState([]);\n\n\tuseEffect(() => {\n\t\tcallProducts()\n\t\t.then((response) =>{\n\t\tconsole.log(response.data);\n\t\tsetProducts(response.data)\n\t\t})\n\t\t.catch(() =>{});\n\t}, []);\n\n\treturn (\n\t\t<CartProvider>\n\t\t\t<div className='all-cards'>\n\t\t\t\t{products.map((product)=>(\n\t\t\t\t\t<div className='each-card' key={product.id}>\n\t\t\t\t\t<ul className='items'>\n\t\t\t\t\t\t<div className='image-item'>\n\t\t\t\t\t\t\t<img src={product.image} alt='Item'></img>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t<li className='item-name'>{product.name}</li>\n\t\t\t\t\t\t<li>Precio: ${product.price}</li>\n\t\t\t\t\t</ul>\n\t\t\t\t\t<button className='add-btn' onClick={() => addItemToCart(product)}>Add To Cart</button>\n\t\t\t\t</div>\n\t\t\t\t))}\n\t\t\t</div>\n\t\t</CartProvider>\n\t\t// <div>\n\t\t// \t<CartOrder\n        //       selectedProduct={selectedProduct}\n        //       vSumTotal={vSumTotal}\n        //       setVSumTotal={setVSumTotal}\n        //       setSelectedProduct={setSelectedProduct}\n        //     />\n\t\t// \t{productos.map((producto)=>(\n\t\t// \t\t<div className='each-card' key={producto.id}>\n\t\t// \t\t<ul className='items'>\n\t\t// \t\t\t<div className='image-item'>\n\t\t// \t\t\t\t<img src={producto.image} alt='Item'></img>\n\t\t// \t\t\t</div>\n\t\t// \t\t\t<li className='item-name'>{producto.name}</li>\n\t\t// \t\t\t<li>Precio: ${producto.price}</li>\n\t\t// \t\t</ul>\n\t\t// \t\t<button className='add-btn' onClick={() => (selectedProduct.producto.price) (vSumTotal.producto.price)}>Agregar al carrito</button>\n\t\t// \t</div>\n\t\t// \t))}\n\t\t// </div>\n\t);\n}\n\nexport default Card;\n","/Users/paolahuyo/PROYECTOS-LABORATORIA/BOG004-burger-queen-api-client/src/pages/Home.jsx",[],[],"/Users/paolahuyo/PROYECTOS-LABORATORIA/BOG004-burger-queen-api-client/src/api/Products.js",[],[],"/Users/paolahuyo/PROYECTOS-LABORATORIA/BOG004-burger-queen-api-client/src/Context/CartContext.jsx",["103","104","105"],[],"import React, { createContext, useEffect, useState } from \"react\";\n\nexport const CartContext = createContext();\n\nexport const CartProvider = ({children}) => {\n    const [cartItems, setCartItems] = useState (() => {\n        try {\n            const productsLocalStorage = localStorage.getItem('cartProducts');\n            return productsLocalStorage ? JSON.parse(productsLocalStorage) : [];\n        } catch (err) {\n            return []\n        }\n    })\n\n    useEffect(() => {\n        localStorage.setItem('cartProducts', JSON.stringify(cartItems));\n        console.log(cartItems);\n    }, [cartItems]);\n\n    const addItemToCart = (product) => {\n        const inCart = cartItems.find(\n        (productInCart)=> productInCart.id === product.id\n        );\n        if(inCart){\n            setCartItems(\n                cartItems.map((productInCart)=>{\n                    if(productInCart.id === product.id){\n                        return{... inCart, amount:inCart.amount + 1};\n                    } else return productInCart;\n                })\n            );\n        } else {\n        setCartItems([... cartItems,{... product, amount:1}]);\n        }\n    }\n\n    const deleteItemToCart = (product) => {\n        const inCart = cartItems.find(\n            (productInCart) => productInCart.id === product.id\n        );\n        if (inCart.amount === 1) {\n            setCartItems(\n                cartItems.filter((productInCart) => productInCart.id !== product.id)\n            );\n        } else {\n            setCartItems(\n                cartItems.map((productInCart) => {\n                    if(productInCart.id === product.id ) {\n                        return {...inCart, amount: inCart.amount - 1}\n                    } else return productInCart\n                }));\n            }\n    };\n\n    return (\n        <CartContext.Provider value = {{cartItems, addItemToCart, deleteItemToCart}}>\n            {children}\n        </CartContext.Provider>\n    );\n};\n\nexport default CartContext;","/Users/paolahuyo/PROYECTOS-LABORATORIA/BOG004-burger-queen-api-client/src/components/Cart.jsx",[],[],"/Users/paolahuyo/PROYECTOS-LABORATORIA/BOG004-burger-queen-api-client/src/components/ItemCart.jsx",["106","107","108","109"],[],"/Users/paolahuyo/PROYECTOS-LABORATORIA/BOG004-burger-queen-api-client/src/components/CartOrder.jsx",[],[],{"ruleId":"110","severity":1,"message":"111","line":6,"column":8,"nodeType":"112","messageId":"113","endLine":6,"endColumn":17},{"ruleId":"110","severity":1,"message":"114","line":12,"column":9,"nodeType":"112","messageId":"113","endLine":12,"endColumn":18},{"ruleId":"110","severity":1,"message":"115","line":12,"column":20,"nodeType":"112","messageId":"113","endLine":12,"endColumn":32},{"ruleId":"110","severity":1,"message":"116","line":13,"column":9,"nodeType":"112","messageId":"113","endLine":13,"endColumn":24},{"ruleId":"110","severity":1,"message":"117","line":13,"column":26,"nodeType":"112","messageId":"113","endLine":13,"endColumn":44},{"ruleId":"118","severity":1,"message":"119","line":28,"column":35,"nodeType":"120","messageId":"121","endLine":28,"endColumn":36,"fix":"122"},{"ruleId":"118","severity":1,"message":"123","line":33,"column":26,"nodeType":"120","messageId":"121","endLine":33,"endColumn":27,"fix":"124"},{"ruleId":"118","severity":1,"message":"119","line":33,"column":41,"nodeType":"120","messageId":"121","endLine":33,"endColumn":42,"fix":"125"},{"ruleId":"126","severity":1,"message":"127","line":9,"column":21,"nodeType":"128","endLine":9,"endColumn":32},{"ruleId":"126","severity":1,"message":"127","line":10,"column":21,"nodeType":"128","endLine":10,"endColumn":32},{"ruleId":"126","severity":1,"message":"127","line":11,"column":21,"nodeType":"128","endLine":11,"endColumn":32},{"ruleId":"126","severity":1,"message":"127","line":12,"column":21,"nodeType":"128","endLine":12,"endColumn":32},"no-unused-vars","'CartOrder' is defined but never used.","Identifier",