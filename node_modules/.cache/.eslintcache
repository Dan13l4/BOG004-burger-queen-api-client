           cartItems.filter((productInCart) => productInCart.id !== product.id)\n            );\n        } else {\n            setCartItems(\n                cartItems.map((productInCart) => {\n                    if(productInCart.id === product.id ) {\n                        return {...inCart, amount: inCart.amount - 1}\n                    } else return productInCart\n                }));\n            }\n    };\n\n    return (\n        <CartContext.Provider value = {{cartItems, addItemToCart, deleteItemToCart}}>\n            {children}\n        </CartContext.Provider>\n    );\n};\n\nexport default CartContext;","/Users/paolahuyo/PROYECTOS-LABORATORIA/BOG004-burger-queen-api-client/src/components/Cart.jsx",["104"],[],"/Users/paolahuyo/PROYECTOS-LABORATORIA/BOG004-burger-queen-api-client/src/components/ItemCart.jsx",["105","106","107","108"],[],{"ruleId":"109","severity":1,"message":"110","line":1,"column":38,"nodeType":"111","messageId":"112","endLine":1,"endColumn":48},{"ruleId":"113","severity":1,"message":"114","line":44,"column":17,"nodeType":"115","endLine":44,"endColumn":51},{"ruleId":"116","severity":1,"message":"117","line":60,"column":1,"nodeType":"118","messageId":"119","endLine":75,"endColumn":11},{"ruleId":"109","severity":1,"message":"120","line":11,"column":9,"nodeType":"111","messageId":"112","endLine":11,"endColumn":18},{"ruleId":"109","severity":1,"message":"121","line":11,"column":20,"nodeType":"111","messageId":"112","endLine":11,"endColumn":32},{"ruleId":"109","severity":1,"message":"122","line":12,"column":9,"nodeType":"111","messageId":"112","endLine":12,"endColumn":24},{"ruleId":"109","severity":1,"message":"123","line":12,"column":26,"nodeType":"111","messageId":"112","endLine":12,"endColumn":44},{"ruleId":"124","severity":1,"message":"125","line":28,"column":35,"nodeType":"126","messageId":"127","endLine":28,"endColumn":36,"fix":"128"},{"ruleId":"124","severity":1,"message":"129","line":33,"column":26,"nodeType":"126","messageId":"127","en